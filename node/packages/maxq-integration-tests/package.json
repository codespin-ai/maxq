{
  "name": "@agilehead/maxq-integration-tests",
  "version": "0.0.10",
  "description": "Integration tests for MaxQ",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit",
    "test:build": "tsc --noEmit",
    "test": "LOG_LEVEL=error mocha --timeout 120000",
    "test:grep": "LOG_LEVEL=error mocha --timeout 120000 --grep",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "maxq": "file:../maxq",
    "maxq-test-utils": "file:../maxq-test-utils"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/mocha": "^10.0.10",
    "@types/chai": "^5.2.2",
    "@types/chai-as-promised": "^8.0.1",
    "@types/sinon": "^17.0.3",
    "mocha": "^11.0.1",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.0",
    "sinon": "^19.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
